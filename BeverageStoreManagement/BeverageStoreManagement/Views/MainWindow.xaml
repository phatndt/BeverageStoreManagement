                                     Tag="HomeMinusOutline"
                                     Content="Home"
                                     Uid="0"
                                     Command="{Binding SwitchTabCommand}"
                                     CommandParameter="{Binding ElementName=wdMain}">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="MouseMove">
                                    <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                           CommandParameter="{Binding ElementName=rdHome}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <RadioButton.ToolTip>
                                <ToolTip Content="Home"
                                         Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                            </RadioButton.ToolTip>
