<Window x:Class="BeverageStoreManagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:BeverageStoreManagement"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
        xmlns:uc="clr-namespace:BeverageStoreManagement.Resources.UserControls"
        xmlns:Wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        x:Name="wdMain"
        Height="800"
        Width="1500"
        Icon="/BeverageStoreManagement;component/Resources/Images/logo.png"
        WindowStartupLocation="CenterScreen"
        Background="Transparent"
        WindowStyle="None"
        AllowsTransparency="True"
        DataContext="{StaticResource HomeVM}">
    <Border CornerRadius="8"
            Background="{DynamicResource PrimaryBackgroundColor}">
        <Grid Height="800"
              VerticalAlignment="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="1320" />
            </Grid.ColumnDefinitions>
            <Border CornerRadius="8 0 0 8"
                    Background="{DynamicResource SecundaryBackgroundColor}">
                <Grid x:Name="gridMenu">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="80" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid Width="40"
                          Height="40">
                        <Image Source="/BeverageStoreManagement;component/Resources/Images/logo.png" />
                    </Grid>
                    <ScrollViewer Grid.Row="1"
                                  VerticalScrollBarVisibility="Auto"
                                  HorizontalScrollBarVisibility="Disabled">
                        <StackPanel>
                            <!--0 Home-->
                            <Expander x:Name="expanderHome">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal"
                                                Height="30">
                                        <TextBlock Text="Home"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <Button x:Name="rdHome"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="HomeMinusOutline"
                                            Content="Home"
                                            Uid="0"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdHome}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Home"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                    <Button x:Name="rdSale"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="HomeModern"
                                            Content="Sale"
                                            Uid="1"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdSale}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Sale"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>
                            <!--1 -->
                            <Expander x:Name="expanderStore">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock Text="Store"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <Button x:Name="rdInvoice"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="Account"
                                            Content="Invoice"
                                            Uid="2"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdInvoice}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Invoice"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>

                            <!--3-->
                            <Expander x:Name="expanderWarehouse">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock Text="Warehouse"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <Button x:Name="rdProduct"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="GoogleMyBusiness"
                                            Content="Product"
                                            Uid="3"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdProduct}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Product"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>

                                    <!--4-->
                                    <Button x:Name="rdMaterial"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="HomePlus"
                                            Content="Material"
                                            Uid="4"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdMaterial}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Material"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                    <!--5-->
                                    <Button x:Name="rdImportBill"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="GlassTulip"
                                            Content="Import Bill"
                                            Uid="5"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdImportBill}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Import Bill"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>

                            <Expander x:Name="expanderPartner">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock Text="Partner"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <Button x:Name="rdSupplier"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="TruckOutline"
                                            Content="Supplier"
                                            Uid="12"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdSupplier}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Dolly"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>

                            <!--6-->
                            <Expander  x:Name="expanderVoucher">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock Text="Voucher"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <Button x:Name="rdPaymentVoucher"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="Receipt"
                                            Content="Payment"
                                            Uid="6"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdPaymentVoucher}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Payment Voucher"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>

                                    <!--7-->
                                    <Button x:Name="rdReceiptVoucher"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="Receipt"
                                            Content="Receipt"
                                            Uid="7"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdReceiptVoucher}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Receipt Voucher"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>

                            <Expander  x:Name="expanderManage">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Manage"
                                                   TextAlignment="Center"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="18"
                                                   FontWeight="SemiBold"
                                                   Foreground="{StaticResource PrimaryGrayColor}" />
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>
                                    <!--8-->
                                    <Button x:Name="rdIncident"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="PaperRollOutline"
                                            Content="Incident"
                                            Uid="8"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdIncident}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Incident"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>

                                    <!--9-->
                                    <Button x:Name="rdEmployee"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="BadgeAccountHorizontalOutline"
                                            Content="Employee"
                                            Uid="9"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdEmployee}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Employee"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                    <!--10-->
                                    <Button x:Name="rdAcount"
                                            Style="{DynamicResource MenuRadioButtonStyle}"
                                            Tag="Account"
                                            Content="Account"
                                            Uid="10"
                                            Command="{Binding SwitchTabCommand}"
                                            CommandParameter="{Binding ElementName=wdMain}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="MouseMove">
                                                <i:InvokeCommandAction Command="{Binding GetUidCommand}"
                                                                       CommandParameter="{Binding ElementName=rdAcount}" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <Button.ToolTip>
                                            <ToolTip Content="Account Management"
                                                     Style="{DynamicResource ToolTipStyleMainMenuButton}"></ToolTip>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </Expander>
                        </StackPanel>
                    </ScrollViewer>
                    <Grid Grid.Row="3"
                          VerticalAlignment="Bottom"
                          Margin="0 0 0 20">
                        <Button Style="{StaticResource BlueButton}"
                                Width="120"
                                Height="45"
                                Content="Logout"
                                Command="{Binding LogoutCommand}"
                                CommandParameter="{Binding ElementName=wdMain}" />
                        <!--<Ellipse Width="40"
                                 Height="40"
                                 HorizontalAlignment="Center"
                                 VerticalAlignment="Center">
                            <Ellipse.Fill>
                                <ImageBrush ImageSource="/BeverageStoreManagement;component/Resources/Images/logo.png" />
                            </Ellipse.Fill>
                        </Ellipse>-->
                    </Grid>
                </Grid>

            </Border>
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="50" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <!-- Start: Button Close | Restore | Minimize -->
                <StackPanel Grid.Row="0"
                            Grid.Column="4"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Margin="0,5,10,0"
                            Height="40"
                            Orientation="Horizontal"
                            FlowDirection="RightToLeft">
                    <Button x:Name="btnClose"
                            Style="{StaticResource IconButtonsStyle}"
                            Tag="IsCloseButton"
                            Content="WindowClose"
                            Click="btnClose_Click" />
                    <Button x:Name="btnRestore"
                            Margin="5,0"
                            IsEnabled="False">
                        <Button.Style>
                            <Style TargetType="{x:Type Button}"
                                   BasedOn="{StaticResource IconButtonsStyle}">
                                <Setter Property="Content"
                                        Value="WindowMaximize" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=home, Path=WindowState}"
                                                 Value="Maximized">
                                        <Setter Property="Content"
                                                Value="WindowMaximize" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                    <Button x:Name="btnMinimize"
                            Style="{DynamicResource IconButtonsStyle}"
                            Content="WindowMinimize"
                            Click="btnMinimize_Click" />

                </StackPanel>

                <Grid Margin="10"
                      Grid.Row="1">
                    <!--0-->
                    <Border CornerRadius="8"
                            Background="{DynamicResource White}"
                            x:Name="grdHome"
                            Visibility="Collapsed">
                        <Grid x:Name="grdBody_Home"
                              Grid.Row="1"
                              DataContext="{StaticResource ReportVM}"
                              Visibility="Visible">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="200" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="Loaded">
                                    <i:InvokeCommandAction Command="{Binding LoadCommand}"
                                                           CommandParameter="{Binding ElementName=wdMain}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Border Padding="4"
                                        Margin="20 20 20 20"
                                        Grid.Column="0"
                                        CornerRadius="4">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0">
                                            <GradientStop Color="#FF22EBD2"
                                                          Offset="1" />
                                            <GradientStop Color="#FFD0D118" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                    <materialDesign:Card Background="Transparent">
                                        <Grid>
                                            <Label x:Name="label"
                                                   Content="Total money in month"
                                                   Style="{StaticResource ReportTitle}" />
                                            <Label x:Name="label_total_money_business"
                                                   Content="{Binding ThisMonthMoney}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 70 0 0"
                                                   VerticalAlignment="Top"
                                                   FontSize="24"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label Content="{Binding ThisMonth}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 100 0 0"
                                                   VerticalAlignment="Bottom"
                                                   FontSize="18"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label x:Name="label_image_business"
                                                   Content=""
                                                   HorizontalAlignment="Right"
                                                   Height="60"
                                                   Margin="10 10"
                                                   VerticalAlignment="Top"
                                                   Width="60">
                                                <Label.Background>
                                                    <ImageBrush ImageSource="/BeverageStoreManagement;component/Resources/Images/money.png"
                                                                Stretch="Uniform" />
                                                </Label.Background>
                                            </Label>
                                        </Grid>
                                    </materialDesign:Card>
                                </Border>
                                <Border Padding="4"
                                        Margin="20 20 20 20"
                                        Grid.Column="1"
                                        CornerRadius="4">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0">
                                            <GradientStop Color="#FF016CFD" />
                                            <GradientStop Color="#FF00E4FF"
                                                          Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                    <materialDesign:Card  Background="Transparent">

                                        <Grid>
                                            <Label x:Name="label_service"
                                                   Content="Compared to last month"
                                                   Style="{StaticResource ReportTitle}" />
                                            <Label x:Name="label_total_money_service"
                                                   Content="{Binding IncreasingPercent}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 70 0 0"
                                                   VerticalAlignment="Top"
                                                   FontSize="24"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label Content="{Binding ThisMonth}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 100 0 0"
                                                   VerticalAlignment="Bottom"
                                                   FontSize="18"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label x:Name="label_image_service"
                                                   Content=""
                                                   HorizontalAlignment="Right"
                                                   Height="60"
                                                   Margin="10 10"
                                                   VerticalAlignment="Top"
                                                   Width="60">
                                                <Label.Background>
                                                    <ImageBrush ImageSource="/BeverageStoreManagement;component/Resources/Images/IncreaseIcon.png"
                                                                Stretch="Uniform" />
                                                </Label.Background>
                                            </Label>
                                        </Grid>
                                    </materialDesign:Card>
                                </Border>
                                <Border Padding="4"
                                        Margin="20 20 20 20"
                                        Grid.Column="2"
                                        CornerRadius="4">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0">
                                            <GradientStop Color="#FFF04662" />
                                            <GradientStop Color="#FFDFAB7E"
                                                          Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                    <materialDesign:Card  Background="Transparent">
                                        <Grid>
                                            <Label x:Name="label_three"
                                                   Content="Order"
                                                   Style="{StaticResource ReportTitle}" />
                                            <Label x:Name="label_total_money_three"
                                                   Content="{Binding NumOfBuy}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 70 0 0"
                                                   VerticalAlignment="Top"
                                                   FontSize="24"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label Content="{Binding ThisMonth}"
                                                   HorizontalAlignment="Left"
                                                   Margin="10 100 0 0"
                                                   VerticalAlignment="Bottom"
                                                   FontSize="18"
                                                   Foreground="#DDF9F9F9"
                                                   FontWeight="Bold" />
                                            <Label x:Name="label_image_three"
                                                   Content=""
                                                   HorizontalAlignment="Right"
                                                   Height="60"
                                                   Margin="10 10"
                                                   VerticalAlignment="Top"
                                                   Width="60">
                                                <Label.Background>
                                                    <ImageBrush ImageSource="/BeverageStoreManagement;component/Resources/Images/IncreaseIcon.png"
                                                                Stretch="Uniform" />
                                                </Label.Background>
                                            </Label>
                                        </Grid>
                                    </materialDesign:Card>
                                </Border>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="380" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <materialDesign:Card Padding="8"
                                                     Margin="20 20 20 20"
                                                     Grid.Column="0">
                                    <Wpf:PieChart x:Name="lvcPieChart"
                                                  DataContext="{StaticResource ReportVM}"
                                                  LegendLocation="None"
                                                  HorizontalAlignment="Center"
                                                  VerticalAlignment="Top"
                                                  Width="300"
                                                  Height="320"
                                                  Margin="0 50 0 0"
                                                  Hoverable="True"
                                                  HoverPushOut="10"
                                                  FontSize="16"
                                                  Series="{Binding PieSeriesCollection}">
                                    </Wpf:PieChart>
                                </materialDesign:Card>
                                <ComboBox x:Name="cboSelectTimePie"
                                          Grid.Column="0"
                                          materialDesign:ComboBoxAssist.ClassicMode="true"
                                          FontSize="16"
                                          FontWeight="Medium"
                                          Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                          materialDesign:HintAssist.Hint="Select time"
                                          materialDesign:HintAssist.Foreground="{DynamicResource Blue2}"
                                          materialDesign:TextFieldAssist.UnderlineBrush="{DynamicResource Blue2}"
                                          Width="140"
                                          Height="45"
                                          HorizontalAlignment="Left"
                                          HorizontalContentAlignment="Left"
                                          VerticalContentAlignment="Center"
                                          Padding="3,0,0,0"
                                          Margin="30,25,0,0"
                                          VerticalAlignment="Top">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="SelectionChanged">
                                            <i:InvokeCommandAction Command="{Binding InitPieChartCommand}"
                                                                   CommandParameter="{Binding ElementName=wdMain}" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ComboBoxItem Content="Today" />
                                    <ComboBoxItem Content="This month" />
                                </ComboBox>
                                <WrapPanel Orientation="Horizontal"
                                           HorizontalAlignment="Center"
                                           Height="100"
                                           Width="280"
                                           Margin="0,400,0,0"
                                           VerticalAlignment="Top"
                                           Grid.Column="0">
                                    <ToggleButton Style="{StaticResource MaterialDesignActionToggleButton}"
                                                  ToolTip="Invoice"
                                                  Width="45"
                                                  Height="45"
                                                  HorizontalAlignment="Left"
                                                  VerticalAlignment="Top"
                                                  Margin="0"
                                                  Background="{DynamicResource PaletteColor3}">
                                        <materialDesign:ToggleButtonAssist.OnContent>
                                            <materialDesign:PackIcon Kind="ReceiptRoll"
                                                                     Width="30"
                                                                     Height="30"
                                                                     Foreground="{DynamicResource White}" />
                                        </materialDesign:ToggleButtonAssist.OnContent>
                                        <materialDesign:PackIcon Kind="ReceiptRoll"
                                                                 Width="30"
                                                                 Height="30"
                                                                 Foreground="{DynamicResource White}" />
                                    </ToggleButton>
                                    <TextBlock TextWrapping="Wrap"
                                               Text="Invoice"
                                               Margin="8,0,16,0"
                                               FontSize="16"
                                               FontWeight="SemiBold"
                                               VerticalAlignment="Center" />
                                    <ToggleButton Style="{StaticResource MaterialDesignActionToggleButton}"
                                                  ToolTip="Payment VC"
                                                  Width="45"
                                                  Height="45"
                                                  HorizontalAlignment="Right"
                                                  VerticalAlignment="Top"
                                                  Margin="0"
                                                  Background="{DynamicResource Color1}">
                                        <materialDesign:ToggleButtonAssist.OnContent>
                                            <materialDesign:PackIcon Kind="Payment"
                                                                     Width="30"
                                                                     Height="30"
                                                                     Foreground="{DynamicResource White}" />
                                        </materialDesign:ToggleButtonAssist.OnContent>
                                        <materialDesign:PackIcon Kind="Payment"
                                                                 Width="30"
                                                                 Height="30"
                                                                 Foreground="{DynamicResource White}" />
                                    </ToggleButton>
                                    <TextBlock TextWrapping="Wrap"
