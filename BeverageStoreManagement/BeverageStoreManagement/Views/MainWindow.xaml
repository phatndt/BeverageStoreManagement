                                                <ComboBoxItem Content="1"></ComboBoxItem>
                                                <ComboBoxItem Content="1"></ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                    </Grid>
                                </Border>
                                <Border Grid.Row="1"
